[submodule "tmux/.tmux/plugins/tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "alacritty/.config/alacritty/themes"]
	path = alacritty/.config/alacritty/themes
	url = https://github.com/alacritty/alacritty-theme
[submodule "kakoune/.config/kak/kak-bundle"]
	path = kakoune/.config/kak/bundle/kak-bundle
	url = https://github.com/jdugan6240/kak-bundle
[submodule "bin/.bin/moreutils"]
	path = bin/.bin/moreutils
	url = git://git.joeyh.name/moreutils
[submodule "ghostty/.config/ghostty/shaders"]
